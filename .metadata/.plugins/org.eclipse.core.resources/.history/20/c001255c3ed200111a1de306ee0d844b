package com.first.google_m;
import com.google.gson.annotations.SerializedName;
public class Fields {
    @SerializedName("fields")
    public Object fields;
    private String content;
    private int addrX;
    private int addrY;
    private String title;
    private String addr;

    public void setAddr(String arg){
        addr = arg;
    }
    public void setTitle(String arg){
        title = arg;
    }
    public void setContent(String arg){
        content = arg;
    }
    public void setAddrX(String arg){
        addrX = (int)(Float.parseFloat(arg)*1e6);
    }
    public void setAddrY(String arg){
        addrY = (int)(Float.parseFloat(arg)*1e6);
    }
    public String getTitle()
    {
        return title;
    }
    public String getContent()
    {
        return content;
    }
    public int getAddrX()
    {
        return addrX;
    }
    public String getAddr()
    {
        return addr;
    }
    public int getAddrY()
    {
        return addrY;
    }

    //    public Fields()
    //    {
    //        StringTokenizer tk = new StringTokenizer(fields.toString(),"{,}",true);
    //        tk.nextElement();
    //    }

}
